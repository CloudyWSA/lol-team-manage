+        }
+
+        if (type === "youtube" && !url) {
+            toast.error("Por favor, insira a URL do YouTube")
+            return
+        }
+
+        setIsUploading(true)
+        try {
+            let storageId: Id<"_storage"> | undefined
+            let finalUrl: string | undefined = url
