+"use client"
+
+import { useMemo } from "react"
+import { Card, CardContent } from "@/components/ui/card"
+import { Badge } from "@/components/ui/badge"
+import { Users, Trophy, ExternalLink, Calendar } from "lucide-react"
+import { Doc } from "@/convex/_generated/dataModel"
+import { Button } from "@/components/ui/button"
+import Link from "next/link"
+
+interface ScrimTeamsTabProps {
+    scrims: Doc<"scrims">[]
+}
+
+interface TeamStats {
+    name: string
+    totalGames: number
+    wins: number
+    lastGame: Doc<"scrims">
+    winRate: number
+}
+
+export function ScrimTeamsTab({ scrims }: ScrimTeamsTabProps) {
+    const teamStats = useMemo(() => {
+        const statsMap = new Map<string, TeamStats>()
+
+        scrims.forEach((scrim) => {
+            const existing = statsMap.get(scrim.opponent)
+            const isWin = scrim.won === true
+
+            if (existing) {
+                existing.totalGames += 1
+                existing.wins += isWin ? 1 : 0
+                // Update last game if current scrim is newer (assuming chronological order or simple comparison)
+                // Since we get them from Convex desc, the first one seen is the most recent
+            } else {
+                statsMap.set(scrim.opponent, {
+                    name: scrim.opponent,
+                    totalGames: 1,
+                    wins: isWin ? 1 : 0,
+                    lastGame: scrim,
+                    winRate: 0,
+                })
+            }
+        })
+
+        return Array.from(statsMap.values()).map(stat => ({
+            ...stat,
+            winRate: Math.round((stat.wins / stat.totalGames) * 100)
+        })).sort((a, b) => b.totalGames - a.totalGames)
+    }, [scrims])
+
+    if (teamStats.length === 0) {
+        return (
+            <div className="flex flex-col items-center justify-center h-60 border-2 border-dashed rounded-3xl opacity-50">
+                <Users className="h-10 w-10 mb-2" />
+                <p className="font-bold">Nenhum adversário encontrado</p>
+                <p className="text-xs">O histórico de times será populado com base nas suas scrims concluídas.</p>
+            </div>
+        )
+    }
+
+    return (
+        <div className="grid gap-4 md:grid-cols-2">
+            {teamStats.map((stat) => (
+                <Card key={stat.name} className="stat-card border-border/50 hover:border-primary/50 transition-all overflow-hidden">
+                    <CardContent className="p-0">
+                        <div className="flex items-stretch h-full">
+                            <div className="w-2 bg-primary/20" />
+                            <div className="flex-1 p-4">
+                                <div className="flex items-center justify-between mb-4">
+                                    <div className="flex items-center gap-3">
+                                        <div className="flex h-10 w-10 items-center justify-center rounded-lg bg-muted">
+                                            <Users className="h-5 w-5 text-muted-foreground" />
+                                        </div>
+                                        <div>
+                                            <h3 className="font-bold text-lg">{stat.name}</h3>
+                                            <p className="text-xs text-muted-foreground">Último jogo: {stat.lastGame.date}</p>
+                                        </div>
+                                    </div>
+                                    <Badge variant={stat.winRate >= 50 ? "default" : "outline"} className="h-fit">
+                                        {stat.winRate}% WR
+                                    </Badge>
+                                </div>
+
+                                <div className="grid grid-cols-3 gap-2">
+                                    <div className="text-center p-2 rounded-lg bg-muted/30">
+                                        <p className="text-[10px] uppercase text-muted-foreground font-bold">Jogos</p>
+                                        <p className="font-black text-primary">{stat.totalGames}</p>
+                                    </div>
+                                    <div className="text-center p-2 rounded-lg bg-muted/30">
+                                        <p className="text-[10px] uppercase text-muted-foreground font-bold">Vitórias</p>
+                                        <p className="font-black text-green-500">{stat.wins}</p>
+                                    </div>
+                                    <div className="text-center p-2 rounded-lg bg-muted/30">
+                                        <p className="text-[10px] uppercase text-muted-foreground font-bold">Derrotas</p>
+                                        <p className="font-black text-red-500">{stat.totalGames - stat.wins}</p>
+                                    </div>
+                                </div>
+
+                                <div className="mt-4 flex items-center justify-between pt-4 border-t border-border/50">
+                                    <div className="flex items-center gap-2">
+                                        <Trophy className={`h-4 w-4 ${stat.lastGame.won ? "text-yellow-500" : "text-muted-foreground/30"}`} />
+                                        <span className="text-xs font-medium">
+                                            Status recente: <span className={stat.lastGame.won ? "text-green-500" : "text-red-500"}>
+                                                {stat.lastGame.won ? "Vitória" : "Derrota"} ({stat.lastGame.result})
+                                            </span>
+                                        </span>
+                                    </div>
+                                    <Button variant="ghost" size="sm" asChild className="h-8 px-2 text-xs">
+                                        <Link href={`/scrims/${stat.lastGame._id}`}>
+                                            Ver Detalhes <ExternalLink className="ml-1 h-3 w-3" />
+                                        </Link>
+                                    </Button>
+                                </div>
+                            </div>
+                        </div>
+                    </CardContent>
+                </Card>
+            ))}
+        </div>
+    )
+}
