new file mode 100644
index 0000000..d3166a9
--- /dev/null
+++ b/components/scrims/scrim-analysis-tab.tsx
@@ -0,0 +1,160 @@
+"use client"
+
+import { useMemo } from "react"
+import { Card, CardContent, CardHeader, CardTitle } from "@/components/ui/card"
+import {
+    BarChart,
+    Bar,
+    XAxis,
+    YAxis,
+    CartesianGrid,
+    Tooltip,
+    ResponsiveContainer,
+    LineChart,
+    Line,
+    Cell
+} from "recharts"
+import { BarChart3, TrendingUp, Target, Swords, PieChart } from "lucide-react"
+import { Doc } from "@/convex/_generated/dataModel"
+
+interface ScrimAnalysisTabProps {
+    scrims: Doc<"scrims">[]
+}
+
+export function ScrimAnalysisTab({ scrims }: ScrimAnalysisTabProps) {
+    const stats = useMemo(() => {
+        if (scrims.length === 0) return null
+
+        const total = scrims.length
+        const wins = scrims.filter(s => s.won).length
+        const winRate = Math.round((wins / total) * 100)
+
+        // Stats by format
+        const formats = ["BO1", "BO3", "BO5"]
+        const formatData = formats.map(f => {
+            const filtered = scrims.filter(s => s.format === f)
+            const count = filtered.length
+            const fWins = filtered.filter(s => s.won).length
+            return {
+                name: f,
+                games: count,
+                winRate: count > 0 ? Math.round((fWins / count) * 100) : 0
+            }
+        })
+
+        // Trend data (last 10 scrims)
+        const trendData = [...scrims].reverse().slice(-10).map((s, i) => ({
+            name: `S${i + 1}`,
+            result: s.won ? 1 : 0,
+            label: s.opponent
+        }))
+
+        return {
+            winRate,
+            total,
+            wins,
+            losses: total - wins,
+            formatData,
+            trendData
+        }
+    }, [scrims])
+
+    if (!stats) {
+        return (
+            <div className="flex flex-col items-center justify-center h-60 border-2 border-dashed rounded-3xl opacity-50">
+                <BarChart3 className="h-10 w-10 mb-2" />
+                <p className="font-bold">Dados insuficientes para análise</p>
+                <p className="text-xs">Conclua algumas scrims para visualizar as estatísticas de performance.</p>
+            </div>
+        )
+    }
+
+    return (
+        <div className="space-y-6">
+            <div className="grid gap-4 md:grid-cols-2 lg:grid-cols-4">
+                <MetricCard title="Taxa de Vitória" value={`${stats.winRate}%`} icon={Target} sub={`${stats.wins}W - ${stats.losses}L`} />
+                <MetricCard title="Total Scrims" value={stats.total.toString()} icon={Swords} sub="Partidas oficiais de treino" />
+                <MetricCard title="Melhor Formato" value={stats.formatData.reduce((prev, current) => (prev.winRate > current.winRate) ? prev : current).name} icon={PieChart} sub="Maior win rate" />
+                <MetricCard title="Tendência" value={stats.trendData[stats.trendData.length - 1]?.result ? "Alta" : "Recuperação"} icon={TrendingUp} sub="Baseado no último jogo" />
+            </div>
+
+            <div className="grid gap-6 lg:grid-cols-2">
+                <Card className="stat-card border-border/50">
+                    <CardHeader>
+                        <CardTitle className="text-sm font-bold uppercase tracking-wider flex items-center gap-2">
+                            <PieChart className="h-4 w-4 text-primary" /> Performance por Formato
+                        </CardTitle>
+                    </CardHeader>
+                    <CardContent>
+                        <div className="h-[300px] w-full">
+                            <ResponsiveContainer width="100%" height="100%">
+                                <BarChart data={stats.formatData}>
+                                    <CartesianGrid strokeDasharray="3 3" vertical={false} stroke="rgba(255,255,255,0.1)" />
+                                    <XAxis dataKey="name" axisLine={false} tickLine={false} />
+                                    <YAxis axisLine={false} tickLine={false} unit="%" />
+                                    <Tooltip
+                                        contentStyle={{ backgroundColor: "#1a1a1a", border: "1px solid rgba(255,255,255,0.1)", borderRadius: "12px" }}
+                                        cursor={{ fill: "rgba(255,255,255,0.05)" }}
+                                    />
+                                    <Bar dataKey="winRate" radius={[4, 4, 0, 0]}>
+                                        {stats.formatData.map((entry, index) => (
+                                            <Cell key={`cell-${index}`} fill={entry.winRate >= 50 ? "hsl(var(--primary))" : "#ef4444"} />
+                                        ))}
+                                    </Bar>
+                                </BarChart>
+                            </ResponsiveContainer>
+                        </div>
+                    </CardContent>
+                </Card>
+
+                <Card className="stat-card border-border/50">
+                    <CardHeader>
+                        <CardTitle className="text-sm font-bold uppercase tracking-wider flex items-center gap-2">
+                            <TrendingUp className="h-4 w-4 text-primary" /> Histórico de Performance
+                        </CardTitle>
+                    </CardHeader>
+                    <CardContent>
+                        <div className="h-[300px] w-full">
+                            <ResponsiveContainer width="100%" height="100%">
+                                <LineChart data={stats.trendData}>
+                                    <CartesianGrid strokeDasharray="3 3" vertical={false} stroke="rgba(255,255,255,0.1)" />
+                                    <XAxis dataKey="name" axisLine={false} tickLine={false} />
+                                    <YAxis domain={[0, 1]} ticks={[0, 1]} axisLine={false} tickLine={false} tickFormatter={(v) => v === 1 ? "W" : "L"} />
+                                    <Tooltip
+                                        contentStyle={{ backgroundColor: "#1a1a1a", border: "1px solid rgba(255,255,255,0.1)", borderRadius: "12px" }}
+                                        formatter={((value: any) => [value === 1 ? "Vitória" : "Derrota", "Resultado"]) as any}
+                                        labelFormatter={((label: any, payload: any[]) => payload[0]?.payload?.label || label) as any}
+                                    />
+                                    <Line
+                                        type="monotone"
+                                        dataKey="result"
+                                        stroke="hsl(var(--primary))"
+                                        strokeWidth={3}
+                                        dot={{ fill: "hsl(var(--primary))", strokeWidth: 2, r: 4 }}
+                                        activeDot={{ r: 6, strokeWidth: 0 }}
+                                    />
+                                </LineChart>
+                            </ResponsiveContainer>
+                        </div>
+                    </CardContent>
+                </Card>
+            </div>
+        </div>
+    )
+}
+
+function MetricCard({ title, value, icon: Icon, sub }: { title: string, value: string, icon: any, sub: string }) {
+    return (
+        <Card className="stat-card border-border/50 relative overflow-hidden group">
+            <CardContent className="p-6">
+                <div className="flex items-center justify-between mb-2">
+                    <p className="text-xs font-bold text-muted-foreground uppercase tracking-wider">{title}</p>
+                    <Icon className="h-4 w-4 text-primary opacity-50 group-hover:opacity-100 transition-opacity" />
+                </div>
+                <p className="text-3xl font-black mb-1">{value}</p>
+                <p className="text-[10px] text-muted-foreground font-medium">{sub}</p>
+            </CardContent>
+            <div className="absolute bottom-0 left-0 h-1 w-0 bg-primary transition-all duration-500 group-hover:w-full" />
+        </Card>
+    )
+}
